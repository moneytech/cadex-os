{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${default}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/cross/lib/gcc/i686-elf/8.2.0/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerArgs": [
                "-fno-pie",
                "-m32",
                "-ffreestanding",
                "-nostdlib"
            ],
            "windowsSdkVersion": "10.0.17134.0",
            "intelliSenseMode": "gcc-x86",
            "cStandard": "c11",
            "cppStandard": "c++17"
        },
        {
            "name": "CadexOS",
            "includePath": [
                "${workspaceFolder}/**",
                "${default}",
                "${workspaceFolder}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [
                "Wall",
                "c",
                "ffreestanding",
                "fno-pie",
                "g"
            ],
            "intelliSenseMode": "gcc-x86",
            "cStandard": "c18",
            "cppStandard": "c++20",
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}